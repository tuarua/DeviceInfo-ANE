<?xml version="1.0" encoding="utf-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/19.0">
    <id>com.tuarua.DeviceInfoANE</id>
	<name>DeviceInfo ANE</name>
	<copyright>This work is licensed under Apache License, Copyright (c) 2019 Tua Rua Ltd.</copyright>
	<versionNumber>1.5.0</versionNumber>
    <platforms>
        <platform name="Android-x86">
            <applicationDeployment>
                <nativeLibrary>classes.jar</nativeLibrary>
                <initializer>com.tuarua.DeviceInfoANE</initializer>
                <finalizer>com.tuarua.DeviceInfoANE</finalizer>
            </applicationDeployment>
        </platform>
		<platform name="Android-ARM"> 
           <applicationDeployment>
				<nativeLibrary>classes.jar</nativeLibrary>
				<initializer>com.tuarua.DeviceInfoANE</initializer>
				<finalizer>com.tuarua.DeviceInfoANE</finalizer>
			</applicationDeployment>
        </platform>
		<platform name="Android-ARM64">
           <applicationDeployment>
				<nativeLibrary>classes.jar</nativeLibrary>
				<initializer>com.tuarua.DeviceInfoANE</initializer>
				<finalizer>com.tuarua.DeviceInfoANE</finalizer>
			</applicationDeployment>
        </platform>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libDeviceInfoANE.a</nativeLibrary>
                <initializer>TRDVIExtInizer</initializer>
                <finalizer>TRDVIExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="iPhone-x86">
            <applicationDeployment>
                <nativeLibrary>libDeviceInfoANE.a</nativeLibrary>
                <initializer>TRDVIExtInizer</initializer>
                <finalizer>TRDVIExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
